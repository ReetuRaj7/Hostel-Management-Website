<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/home3">Home</a></li>
    <li class="breadcrumb-item active" aria-current="page">View User</li>
  </ol>
</nav>

<div class="view-user3 p-5">
  {{#each rows}}
  <div class="row mb-5">
    <div class="col text-center">
      <h3>{{this.res_first_name}}</h3>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Member ID</th>
            <th scope="col">First Name</th>
            <th scope="col">Room Number</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <th scope="row">{{this.mem_id}}</th>
            <td>{{this.mem_first_name}}</td>
            <td>{{this.mem_room_no}}</td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  {{/each}}
</div>
